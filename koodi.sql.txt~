CREATE TABLE Keskustelunavaus (KeskusteluId integer PRIMARY KEY, AiheId integer, Tunnus varchar(30), Otsikko varchar(50), PvmAika varchar(50) NOT NULL, FOREIGN KEY(AiheId) REFERENCES Aihe(AiheId),FOREIGN KEY(Tunnus) REFERENCES Kayttaja(Tunnus));
